{
  "name": "vscode-msb-codegen",
  "publisher": "MSB",
  "displayName": "MSB CodeGen",
  "description": "MSB CodeGen is a self-hosted AI coding assistant that can suggest multi-line code or full functions in real-time.",
  "homepage": "",
  "repository": "",
  "bugs": "",
  "license": "Apache-2.0",
  "version": "1.28.0",
  "keywords": [
    "ai",
    "autocomplete",
    "code completion",
    "copilot",
    "intellisense",
    "intellicode",
    "snippets",
    "MSB"
  ],
  "icon": "assets/MSB.jpg",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/node/extension.js",
  "browser": "./dist/browser/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "msb-codegen.toggleInlineCompletionTriggerMode",
        "title": "Toggle Code Completion Trigger Mode (Automatic/Manual)",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.inlineCompletion.trigger",
        "title": "Trigger Code Completion Manually",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.connectToServer",
        "title": "Connect to Server...",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.updateToken",
        "title": "Update Token...",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.openSettings",
        "title": "Open Settings",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.openMSBCodeGenAgentSettings",
        "title": "Open MSB CodeGen Agent Settings",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.gettingStarted",
        "title": "Quick Start",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.status.resetIgnoredIssues",
        "title": "Reset Ignored Issues",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.addRelevantContext",
        "title": "Add Selection to Chat",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.addFileContext",
        "title": "Add File to Chat",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.explainCodeBlock",
        "title": "Explain This",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.fixCodeBlock",
        "title": "Fix This",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.codeReviewCodeBlock",
        "title": "Code Review",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.generateCodeBlockDoc",
        "title": "Generate Docs",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.generateCodeBlockTest",
        "title": "Generate Tests",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.generateCommitMessage",
        "title": "Generate Commit Message",
        "category": "MSB CodeGen",
        "icon": "$(sparkle)"
      },
      {
        "command": "msb-codegen.chat.generateBranchName",
        "title": "Create Branch with Suggestions",
        "category": "MSB CodeGen",
        "icon": "$(git-branch)"
      },
      {
        "command": "msb-codegen.chat.edit.start",
        "title": "Start Inline Editing",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.edit.stop",
        "title": "Stop Inline Editing",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.edit.accept",
        "title": "Accept Changes",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.edit.discard",
        "title": "Discard Changes",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.createPanel",
        "title": "Open Chat in Editor",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.navigate.newChat",
        "title": "New Chat",
        "icon": "$(add)",
        "enablement": "msb-codegen.chatSidePanelStatus === 'ready'",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.chat.navigate.history",
        "title": "Chat History",
        "icon": "$(history)",
        "enablement": "msb-codegen.chatSidePanelStatus === 'ready'",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.toggleLanguageInlineCompletion",
        "title": "Enable/Disable Inline Completion for Current Language",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.terminal.explainSelection",
        "title": "Explain",
        "category": "MSB CodeGen"
      },
      {
        "command": "msb-codegen.terminal.addSelectionToChat",
        "title": "Add Selection to Chat",
        "category": "MSB CodeGen"
      }
    ],
    "submenus": [
      {
        "id": "msb-codegen.submenu",
        "label": "MSB CodeGen"
      },
      {
        "id": "msb-codegen.terminal.submenu",
        "label": "MSB CodeGen"
      }
    ],
    "menus": {
      "msb-codegen.submenu": [
        {
          "command": "msb-codegen.chat.edit.start",
          "when": "msb-codegen.chatEnabled",
          "group": "msb-codegen.submenu.0edit@1"
        },
        {
          "command": "msb-codegen.chat.explainCodeBlock",
          "when": "editorHasSelection && msb-codegen.chatEnabled",
          "group": "msb-codegen.submenu.1edit@1"
        },
        {
          "command": "msb-codegen.chat.codeReviewCodeBlock",
          "when": "editorHasSelection && msb-codegen.chatEnabled",
          "group": "msb-codegen.submenu.1edit@2"
        },
        {
          "command": "msb-codegen.chat.addRelevantContext",
          "when": "editorHasSelection && msb-codegen.chatEnabled",
          "group": "msb-codegen.submenu.2chat@1"
        },
        {
          "command": "msb-codegen.chat.addFileContext",
          "when": "msb-codegen.chatEnabled",
          "group": "msb-codegen.submenu.2chat@2"
        }
      ],
      "msb-codegen.terminal.submenu": [
        {
          "command": "msb-codegen.terminal.explainSelection",
          "group": "msb-codegen.terminal.action"
        },
        {
          "command": "msb-codegen.terminal.addSelectionToChat",
          "group": "msb-codegen.terminal.context"
        }
      ],
      "commandPalette": [
        {
          "command": "msb-codegen.inlineCompletion.trigger",
          "when": "msb-codegen.inlineCompletionTriggerMode === 'manual' && !editorHasSelection && !inlineSuggestionsVisible"
        },
        {
          "command": "msb-codegen.updateToken",
          "when": "msb-codegen.status === 'unauthorized'"
        },
        {
          "command": "msb-codegen.openMSBCodeGenAgentSettings",
          "when": "!isWeb"
        },
        {
          "command": "msb-codegen.chat.explainCodeBlock",
          "when": "editorHasSelection && msb-codegen.chatEnabled"
        },
        {
          "command": "msb-codegen.chat.fixCodeBlock",
          "when": "false"
        },
        {
          "command": "msb-codegen.chat.generateCodeBlockDoc",
          "when": "editorHasSelection && msb-codegen.chatEnabled"
        },
        {
          "command": "msb-codegen.chat.generateCodeBlockTest",
          "when": "editorHasSelection && msb-codegen.chatEnabled"
        },
        {
          "command": "msb-codegen.chat.generateCommitMessage",
          "when": "msb-codegen.chatEnabled"
        },
        {
          "command": "msb-codegen.chat.generateBranchName",
          "when": "msb-codegen.chatEnabled"
        },
        {
          "command": "msb-codegen.chat.edit.start",
          "when": "msb-codegen.chatEnabled"
        },
        {
          "command": "msb-codegen.chat.edit.stop",
          "when": "false"
        },
        {
          "command": "msb-codegen.chat.edit.accept",
          "when": "false"
        },
        {
          "command": "msb-codegen.chat.edit.discard",
          "when": "false"
        }
      ],
      "editor/context": [
        {
          "submenu": "msb-codegen.submenu",
          "group": "2_tabby"
        }
      ],
      "scm/title": [
        {
          "command": "msb-codegen.chat.generateCommitMessage",
          "when": "msb-codegen.chatEnabled",
          "group": "navigation@-1"
        }
      ],
      "view/title": [
        {
          "command": "msb-codegen.chat.navigate.newChat",
          "when": "view == msb-codegen.chatView",
          "group": "navigation@0"
        },
        {
          "command": "msb-codegen.chat.navigate.history",
          "when": "view == msb-codegen.chatView",
          "group": "navigation@1"
        },
        {
          "command": "msb-codegen.chat.createPanel",
          "when": "view == msb-codegen.chatView",
          "group": "0_new"
        }
      ],
      "terminal/context": [
        {
          "submenu": "msb-codegen.terminal.submenu",
          "group": "2_tabby",
          "when": "msb-codegen.terminalContextEnabled"
        }
      ]
    },
    "walkthroughs": [
      {
        "id": "gettingStarted",
        "title": "Getting Started with MSB CodeGen",
        "description": "Set up MSB CodeGen and explore the features.",
        "steps": [
          {
            "id": "connectToServer",
            "title": "Connect to Server",
            "description": "Connect the MSB CodeGen extension to your server. \n[Connect to Server](command:msb-codegen.connectToServer)",
            "media": {
              "markdown": "assets/walkthroughs/connectToServer.md"
            }
          },
          {
            "id": "codeCompletion",
            "title": "Code Completion",
            "description": "Write code more efficiently.",
            "media": {
              "markdown": "assets/walkthroughs/codeCompletion.md"
            }
          },
          {
            "id": "chat",
            "title": "Chat",
            "description": "Ask any coding questions. \n[Start Chat](command:msb-codegen.chatView.focus)",
            "media": {
              "markdown": "assets/walkthroughs/chat.md"
            }
          },
          {
            "id": "commands",
            "title": "Commands",
            "description": "Explore all [MSB CodeGen Commands](command:workbench.action.quickOpen?%5B%22%3EMSB CodeGen%22%5D). \n[MSB CodeGen Command Palette](command:msb-codegen.commandPalette.trigger)",
            "media": {
              "markdown": "assets/walkthroughs/commands.md"
            }
          },
          {
            "id": "keybindings",
            "title": "Keyboard Shortcuts",
            "description": "MSB CodeGen provides a set of default keyboard shortcuts, but you can also customize them. \n[Keyboard Shortcuts](command:msb-codegen.openKeybindings)",
            "media": {
              "markdown": "assets/walkthroughs/keybindings.md"
            }
          }
        ]
      }
    ],
    "configuration": [
      {
        "title": "MSB CodeGen",
        "properties": {
          "msb-codegen.endpoint": {
            "type": "string",
            "default": "http://localhost:8080",
            "pattern": "(^$)|(^https?:\\/\\/\\S+$)",
            "patternErrorMessage": "Please enter a valid HTTP or HTTPS URL.",
            "markdownDescription": "Please use the command [Connect to Server](command:msb-codegen.connectToServer) to specify the endpoint of the MSB CodeGen server."
          },
          "msb-codegen.keybindings": {
            "type": "string",
            "enum": [
              "vscode-style",
              "msb-codegen-style"
            ],
            "default": "vscode-style",
            "markdownDescription": "Select the keybinding profile to accept inline completion. You can also customize it in [Keyboard Shortcuts](command:msb-codegen.openKeybindings). \n | | Next Line | Full Completion | Next Word | \n |:---|:---|:---|:---| \n | _vscode-style_ | - | `Tab` | `Ctrl + RightArrow` (macOS: `Cmd + RightArrow`) | \n | _msb-codegen-style_  _(experimental)_ | `Tab` | `Ctrl + Tab` | `Ctrl + RightArrow` (macOS: `Cmd + RightArrow`) | \n"
          },
          "msb-codegen.settings.advanced": {
            "default": {},
            "properties": {
              "inlineCompletion.triggerMode": {
                "type": "string",
                "enum": [
                  "automatic",
                  "manual"
                ],
                "default": "automatic",
                "description": "Select the code completion trigger mode.",
                "enumDescriptions": [
                  "Automatic trigger when you stop typing",
                  "Manual trigger by pressing `Alt + \\`"
                ]
              },
              "inlineCompletion.disabledLanguages": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "default": [],
                "description": "List of language IDs for which code completion should be disabled. Use the 'Toggle Single Language Inline Completion' command to easily enable or disable completion for the current language."
              },
              "chatEdit.history": {
                "type": "integer",
                "default": 20,
                "description": "The maximum number of recently used commands to keep. Set to 0 to disable recording."
              }
            }
          }
        }
      }
    ],
    "keybindings": [
      {
        "key": "alt+\\",
        "command": "msb-codegen.inlineCompletion.trigger",
        "when": "msb-codegen.inlineCompletionTriggerMode === 'manual' && editorTextFocus && !editorHasSelection && !inlineSuggestionsVisible"
      },
      {
        "command": "msb-codegen.inlineCompletion.accept",
        "key": "tab",
        "when": "config.msb-codegen.keybindings === 'vscode-style' && inlineSuggestionVisible && !editorReadonly && !suggestWidgetVisible && !editorHoverFocused && !editorTabMovesFocus && inlineSuggestionHasIndentationLessThanTabSize"
      },
      {
        "command": "msb-codegen.inlineCompletion.acceptNextWord",
        "key": "ctrl+right",
        "mac": "cmd+right",
        "when": "config.msb-codegen.keybindings === 'vscode-style' && inlineSuggestionVisible && !editorReadonly && !suggestWidgetVisible"
      },
      {
        "command": "msb-codegen.inlineCompletion.accept",
        "key": "ctrl+tab",
        "mac": "ctrl+tab",
        "when": "config.msb-codegen.keybindings === 'msb-codegen-style' && inlineSuggestionVisible && !editorReadonly && !suggestWidgetVisible"
      },
      {
        "command": "msb-codegen.inlineCompletion.acceptNextWord",
        "key": "ctrl+right",
        "mac": "cmd+right",
        "when": "config.msb-codegen.keybindings === 'msb-codegen-style' && inlineSuggestionVisible && !editorReadonly && !suggestWidgetVisible"
      },
      {
        "command": "msb-codegen.inlineCompletion.acceptNextLine",
        "key": "tab",
        "when": "config.msb-codegen.keybindings === 'msb-codegen-style' && inlineSuggestionVisible && !editorReadonly && !suggestWidgetVisible && !editorHoverFocused && !editorTabMovesFocus"
      },
      {
        "command": "msb-codegen.inlineCompletion.dismiss",
        "key": "escape",
        "when": "inlineSuggestionVisible"
      },
      {
        "command": "msb-codegen.chat.edit.start",
        "key": "ctrl+i",
        "mac": "cmd+i",
        "when": "msb-codegen.chatEnabled && editorTextFocus && !editorReadonly"
      },
      {
        "command": "msb-codegen.chat.edit.stop",
        "key": "escape",
        "when": "msb-codegen.chatEditInProgress && editorTextFocus && !editorReadonly"
      },
      {
        "command": "msb-codegen.chat.edit.accept",
        "key": "ctrl+enter",
        "mac": "cmd+enter",
        "when": "msb-codegen.chatEditResolving && !msb-codegen.chatEditInProgress && editorTextFocus && !editorReadonly"
      },
      {
        "command": "msb-codegen.chat.edit.discard",
        "key": "escape",
        "when": "msb-codegen.chatEditResolving && !msb-codegen.chatEditInProgress && editorTextFocus && !editorReadonly"
      },
      {
        "command": "msb-codegen.chat.addRelevantContext",
        "key": "ctrl+l",
        "mac": "cmd+l",
        "when": "msb-codegen.chatEnabled && editorTextFocus && editorHasSelection && msb-codegen.chatSidePanelVisible"
      },
      {
        "command": "msb-codegen.chat.toggleFocus",
        "key": "ctrl+l",
        "mac": "cmd+l",
        "when": "(msb-codegen.chatEnabled && editorTextFocus && !msb-codegen.chatSidePanelVisible) || ((!editorTextFocus || (editorTextFocus && !editorHasSelection)) && msb-codegen.chatSidePanelVisible)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "MSBCodeGenChatView",
          "title": "MSB CodeGen",
          "icon": "assets/chat.svg"
        }
      ]
    },
    "views": {
      "MSBCodeGenChatView": [
        {
          "type": "webview",
          "id": "msb-codegen.chatView",
          "name": "MSB CodeGen",
          "visibility": "visible"
        }
      ]
    }
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json --noEmit && tsup --minify",
    "watch": "tsup --watch",
    "vscode:dev": "pnpm run dev:vscode",
    "dev:vscode": "pnpm run watch --platform node --env.LAUNCH_ON_SUCCESS 1",
    "dev:browser": "pnpm run watch --platform browser --env.LAUNCH_ON_SUCCESS 1",
    "lint": "eslint --ext .ts ./src && prettier --check .",
    "lint:fix": "eslint --fix --ext .ts ./src && prettier --write .",
    "vscode:prepackage": "turbo build --force",
    "vscode:package": "vsce package --no-dependencies",
    "vscode:prepublish": "turbo build --force",
    "vscode:publish": "vsce publish --no-dependencies",
    "vscode:publish-prerelease": "vsce publish --no-dependencies --pre-release",
    "ovsx:publish": "ovsx publish --no-dependencies --pat $OVSX_PAT",
    "ovsx:publish-prerelease": "ovsx publish --no-dependencies --pre-release --pat $OVSX_PAT",
    "package": "pnpm run vscode:package",
    "publish": "pnpm run vscode:publish && pnpm run ovsx:publish",
    "publish-prerelease": "pnpm run vscode:publish-prerelease && pnpm run ovsx:publish-prerelease",
    "test": "mocha"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.2",
    "@types/chai": "^4.3.5",
    "@types/deep-equal": "^1.0.4",
    "@types/diff": "^5.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.x",
    "@types/object-hash": "^3.0.0",
    "@types/semver": "^7.5.8",
    "@types/uuid": "^9.0.0",
    "@types/vscode": "^1.82.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-electron": "^2.1.5",
    "@vscode/test-web": "^0.0.63",
    "@vscode/vsce": "^2.15.0",
    "assert": "^2.0.0",
    "debounce": "^2.2.0",
    "chai": "^4.3.11",
    "dedent": "^0.7.0",
    "deep-equal": "^2.2.1",
    "diff": "^5.2.0",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "fs-extra": "^11.1.1",
    "get-installed-path": "^4.0.8",
    "mocha": "^10.2.0",
    "object-hash": "^3.0.0",
    "ovsx": "^0.9.5",
    "prettier": "^3.0.0",
    "semver": "^7.6.0",
    "tabby-agent": "workspace:*",
    "tabby-chat-panel": "workspace:*",
    "tabby-threads": "workspace:*",
    "tsc-watch": "^6.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.2",
    "uuid": "^9.0.0",
    "vscode-languageclient": "^9.0.1"
  }
}